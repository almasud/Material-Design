<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppMaterialTheme">
        <activity
            android:name=".NavigationDrawerActivity"
            android:theme="@style/AppMaterialTheme.TransparentStatusBarTheme" />
        <activity android:name=".CollapsingToolbarActivity" />
        <activity android:name=".AppBarLayoutActivity" />
        <activity android:name=".AppBarCollapsingToolbarActivity" />
        <activity android:name=".FloatingLabelEditTextActivity" />
        <activity android:name=".TransitionActivity" />
        <activity android:name=".SharedElementActivity" />
        <activity android:name=".RippleEffectActivity" />
        <activity android:name=".AnimationActivity" />
        <activity android:name=".BasicComponentActivity" />
        <activity android:name=".ContextualMenuActivity" />
        <activity android:name=".ToolbarAsActionbarActivity" />
        <activity android:name=".StandaloneToolbarActivity" />
        <activity android:name=".ToolbarsActivity" />
        <activity android:name=".ColorPaletteActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>